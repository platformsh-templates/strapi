{
  "extension": [
    ".js",
    ".jsx"
  ],
  "include": [
    "src"
  ],
  "require": [
    "babel-register"
  ],
  "reporter": [
    "html",
    "lcov"
  ],
  "all": true,
  "check-coverage": true,
  "statements": 100,
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "sourceMap": false,
  "instrument": false
}
